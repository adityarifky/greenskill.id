rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    
    // WARNING: These rules are permissive and allow any authenticated user to read/write any data.
    // This is intended for development to unblock UI creation and should be secured
    // before moving to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
